{
  "name": "vue-tailwind-datetime-picker",
  "version": "1.0.3",
  "description": "Datepicker component for vue.js build with Tailwind CSS & dayjs date library",
  "main": "dist/vue-tailwind-datetime-picker.umd.js",
  "module": "dist/vue-tailwind-datetime-picker.esm.js",
  "unpkg": "dist/vue-tailwind-datetime-picker.min.js",
  "browser": {
    "./sfc": "src/vue-tailwind-datetime-picker.vue"
  },
  "scripts": {
    "dev": "vue serve src/vue-tailwind-datetime-picker.vue",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-tailwind-datetime-picker.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-tailwind-datetime-picker.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-tailwind-datetime-picker.min.js",
    "test": "vue serve examples/demo.vue"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "vue-tailwind-picker",
    "vue-tailwind-datepicker",
    "vue-datepicker",
    "vue-tailwindcss",
    "tailwindcss",
    "vue-tailwind"
  ],
  "author": {
    "name": "kenhyuwa",
    "email": "wahyu.dhiraashandy8@gmail.com",
    "url": "https://tailwindesign.com"
  },
  "contributors": [],
  "license": "MIT",
  "private": false,
  "homepage": "https://vue-tailwind-picker.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowmateproject/vue-tailwind-datetime-picker.git"
  },
  "bugs": {
    "url": "https://github.com/rowmateproject/vue-tailwind-datetime-picker/issues",
    "email": "mail@rowmate.org"
  },
  "devDependencies": {
    "acorn": "^8.0.4",
    "dayjs-dynamic-locale": "0.0.7",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "dayjs": "^1.9.3",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "dayjs": "^1.9.3",
    "tailwindcss": "^1.9.2"
  }
}
